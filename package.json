{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "build": "rm -rf dist && tsc",
    "dev": "node -r dotenv/config -r tsconfig-paths/register ./node_modules/.bin/ts-node-dev --respawn --transpile-only ./src/server.ts",
    "start": "npm run build && NODE_PATH=./dist/src node -r dotenv/config ./dist/src/server.js",
    "migrate:generate": "sequelize migration:generate --name",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:down-all": "npx sequelize-cli db:migrate:undo:all",
    "seed:generate": "sequelize seed:generate --name",
    "migrate:seed": "npx sequelize-cli db:seed:all",
    "migrate:seed-down": "npx sequelize-cli db:seed:undo:all",
    "migrate-test": "NODE_ENV=test npx sequelize-cli db:migrate",
    "migrate-prod": "NODE_ENV=production dotenv -e .env.prod npx sequelize-cli db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@socket.io/admin-ui": "^0.5.1",
    "bcrypt": "^5.1.1",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sanitize-html": "^2.11.0",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/sanitize-html": "^2.9.5",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
